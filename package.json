{
  "name": "convert",
  "version": "0.2.1",
  "description": "Simple File Conversion Application",
  "main": "main.js",
  "author": "Mitch Pierias <github.com/MitchPierias>",
  "private": false,
  "homepage": "./",
  "keywords": [
    "Convert",
    "Conversion",
    "React",
    "Electron",
    "Redux",
    "Lottie",
    "ffmpeg"
  ],
  "scripts": {
    "start": "electron .",
    "start:dev": "webpack-dev-server --env development --progress",
    "build": "webpack --env production",
    "dist": "electron-builder",
    "docs": "./node_modules/.bin/typedoc --out ./docs --mode modules"
  },
  "dependencies": {
    "colors": "^1.3.2",
    "ffmpeg-static": "^2.4.0",
    "ffprobe-static": "^3.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "lodash": "^4.17.4",
    "react": "^16.5.x",
    "react-dom": "^16.5.x",
    "react-dropzone": "^3.13.4",
    "react-lottie": "^1.2.3",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.3.x",
    "react-spring": "^5.8.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.22.0",
    "css-loader": "^1.0.0",
    "css-modules": "^0.3.0",
    "electron": "^3.0.11",
    "electron-builder": "^20.38.3",
    "foreman": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.10.0",
    "react-scripts": "^2.1.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "typedoc": "^0.13.0",
    "webpack": "^3.12.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^2.11.3"
  },
  "build": {
    "productName": "Agora",
    "appId": "com.agora.app",
    "copyright": "Copyright 2018 ${author}",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "files": [
      "**/*",
      "build/**/*",
      "assets/**/*",
      "!dist/**/*",
      "!assets/**/*",
      "!spec/**/*",
      "!config/**/*",
      "node_modules/**/*",
      "node_modules/ffmpeg-static/bin/${os}/${arch}/ffmpeg",
      "node_modules/ffmpeg-static/index.js",
      "node_modules/ffmpeg-static/package.json",
      "node_modules/ffprobe-static/bin/${os}/${arch}/ffmpeg",
      "node_modules/ffprobe-static/index.js",
      "node_modules/ffprobe-static/package.json"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "files": [
        "!node_modules/ffmpeg-static/bin/win${/*}",
        "!node_modules/ffmpeg-static/bin/linux${/*}",
        "!node_modules/ffprobe-static/bin/win${/*}",
        "!node_modules/ffprobe-static/bin/linux${/*}"
      ]
    },
    "extends": null
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
